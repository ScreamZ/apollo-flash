{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "member-access": [
            true,
            "no-public"
        ],
        "no-console": {
            "severity": "warning"
        },
        "object-literal-sort-keys": {
            "severity": "error",
            "options": [
                "ignore-case"
            ]
        }
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "build/",
            "node_modules/"
        ]
    }
}